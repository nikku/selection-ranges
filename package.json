  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "chai": "^4.5.0",
    "eslint": "^9.18.0",
    "eslint-plugin-bpmn-io": "^2.1.0",
    "karma": "^6.4.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-firefox-launcher": "^2.1.3",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.1",
    "mocha": "^10.8.2",
    "npm-run-all": "^4.1.2",
    "puppeteer": "^24.1.0",
    "rollup": "^4.30.1",
    "webpack": "^5.97.1"
  },
  "dependencies": {
    "dom-iterator": "^1.0.0"
  },
  "files": [
    "dist"
